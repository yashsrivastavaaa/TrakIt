{
  "expo": {
    "name": "TrakIt",
    "slug": "TrakIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TrakIt.png",
    "scheme": "trakit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TrakIt.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yashsrivastavaaa.TrakIt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/TrakIt.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TrakIt.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "entryPoint": "./app/index.tsx",
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "expo-router": {
      "appDirectory": "./app",
      "index": "index.tsx"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0642b6ae-873f-40fc-bb4f-2d0ff4132d95"
      }
    }
  }
}
