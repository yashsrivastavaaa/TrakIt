{
  "cli": {
    "version": ">= 16.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DB_API": "${EXPO_PUBLIC_DB_API}"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DB_API": "${EXPO_PUBLIC_DB_API}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_DB_API": "${EXPO_PUBLIC_DB_API}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}